<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <rotate android:fromDegrees="-40"
            android:pivotY="50%"
            android:pivotX="2">
            <shape android:shape="line">
                <stroke android:color="@android:color/white"
                    android:width="1dp"/>
            </shape>
        </rotate>
    </item>
    <item>
        <rotate android:fromDegrees="40"
            android:pivotY="50%"
            android:pivotX="2">
            <shape android:shape="line">
                <stroke android:color="@android:color/white"
                    android:width="1dp"/>
            </shape>
        </rotate>
    </item>

</layer-list>